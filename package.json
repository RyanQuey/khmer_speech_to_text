  "scripts": {
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "dev": "npm run env:dev -- webpack-dev-server",
    "local": "npm run env:dev -- webpack-dev-server --public www.local.test:3000 --host 0.0.0.0",
    "clean": "rimraf dist/*",
    "clean:happy": "rimraf .happypack/*",
    "copy": "copyfiles -u 1 public/* public/**/* dist",
    "dist": "npm run copy & webpack --env=dist",
    "deploy-with-functions": "npm run env:dev && npm run clean && npm run dist && firebase deploy",
    "deploy": "npm run env:dev && npm run clean && npm run dist && firebase deploy --except functions",
    "deploy-rules": "firebase deploy --only firestore:rules",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist"
  },
